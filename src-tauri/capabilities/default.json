{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "status"],

  "permissions": [
    "core:default",
    "opener:default",
    "clipboard-manager:default",
    "dialog:default",
    "global-shortcut:default",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister-all",
    "store:default"
  ]

}
